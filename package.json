{
  "name": "easy-spotify",
  "version": "0.0.1",
  "description": "A spotify web api wrapper library.",
  "main": "lib/index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "build:umd:min": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.min.js --mode production",
    "build:umd": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.js --mode development",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "keywords": [
    "js",
    "tdd",
    "library",
    "spotify"
  ],
  "author": "Bruno Lombardi <djfrizer1@gmail.com> (https://brunolombardi.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bruno-lombardi/easy-spotify/issues"
  },
  "homepage": "https://github.com/bruno-lombardi/easy-spotify#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.11.9",
    "mocha": "^5.2.0",
    "node-fetch": "^2.3.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {}
}
